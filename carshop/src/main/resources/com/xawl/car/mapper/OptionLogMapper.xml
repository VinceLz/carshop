<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xawl.car.dao.OptionLogMapper">
	<insert id="insertLog" parameterType="com.xawl.car.domain.OptionLog">
		insert into order_log
		(goodid,ulogin,createdate,content,bankname,result,`type`)
		values (
		#{goodid},
		#{ulogin},
		#{createdate},
		#{content},
		#{bankname},
		#{result},
		#{type}
		)
	</insert>
</mapper>