<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xawl.car.dao.OrderMapper">
	<insert id="insert" parameterType="com.xawl.car.domain.Order">
		insert into db_order (ordertime,total,status,uid,uname,bid,bname,bphone,uphone,mname,mid,gid,gname,color,buyWay,city,cardCity)
		values(
		#{ordertime},
		#{total},
		#{status},
		#{uid},
		#{uname},
		#{bid},
		#{bname},
		#{bphone},
		#{uphone},
		#{mname},
		#{mid},
		#{gid},
		#{gname},
		#{color},
		#{buyWay},
		#{city},
		#{cardCity}
		)
	</insert>
</mapper>